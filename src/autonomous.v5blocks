l required headers are included.\n#include <stdio.h>\n#include <stdlib.h>\n#include <stdbool.h>\n#include <math.h>\n#include <string.h>\n\n\n#include \"vex.h\"\n\nusing namespace vex;\n\n// Brain should be defined by default\nbrain Brain;\n\n\n// START V5 MACROS\n#define waitUntil(condition)                                                   \\\n  do {                                                                         \\\n    wait(5, msec);                                                             \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END V5 MACROS\n\n\n// Robot configuration code.\nmotor Motor1 = motor(PORT1, ratio18_1, false);\n\nmotor Motor10 = motor(PORT10, ratio18_1, true);\n\nmotor Motor11 = motor(PORT11, ratio18_1, false);\n\nmotor Motor20 = motor(PORT20, ratio18_1, true);\n\ncontroller Controller1 = controller(primary);\nmotor launcherMotorA = motor(PORT5, ratio6_1, false);\nmotor launcherMotorB = motor(PORT6, ratio6_1, true);\nmotor_group launcher = motor_group(launcherMotorA, launcherMotorB);\n\n\n\n\n\n\n// Helper to make playing sounds from the V5 in VEXcode easier and\n// keeps the code cleaner by making it clear what is happening.\nvoid playVexcodeSound(const char *soundName) {\n  printf(\"VEXPlaySound:%s\\n\", soundName);\n  wait(5, msec);\n}\n\n\n\n// Generated code.\n\n\n\n// define variable for remote controller enable/disable\nbool RemoteControlCodeEnabled = true;\n// define variables used for controlling motors based on controller inputs\nbool Controller1RightShoulderControlMotorsStopped = true;\n\n// define a task that will handle monitoring inputs from Controller1\nint rc_auto_loop_function_Controller1() {\n  // process the controller input every 20 milliseconds\n  // update the motors based on the input values\n  while(true) {\n    if(RemoteControlCodeEnabled) {\n      // check the ButtonR1/ButtonR2 status to control launcher\n      if (Controller1.ButtonR1.pressing()) {\n        launcher.spin(forward);\n        Controller1RightShoulderControlMotorsStopped = false;\n      } else if (Controller1.ButtonR2.pressing()) {\n        launcher.spin(reverse);\n        Controller1RightShoulderControlMotorsStopped = false;\n      } else if (!Controller1RightShoulderControlMotorsStopped) {\n        launcher.stop();\n        // set the toggle so that we don't constantly tell the motor to stop when the buttons are released\n        Controller1RightShoulderControlMotorsStopped = true;\n      }\n    }\n    // wait before repeating the process\n    wait(20, msec);\n  }\n  return 0;\n}\n\ntask rc_auto_loop_task_Controller1(rc_auto_loop_function_Controller1);\n\n// Include the V5 Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nfloat myVariable;\n\nevent Up = event();\nevent message1 = event();\nevent Down = event();\nevent Right = event();\nevent Left = event();\nevent Clockwise = event();\nevent Counter_Clockwise = event();\nevent STOP = event();\n\n// \"when started\" hat block\nint whenStarted1() {\n  launcher.setVelocity(100.0, percent);\n  Motor1.setVelocity(100.0, percent);\n  Motor20.setVelocity(100.0, percent);\n  Motor11.setVelocity(100.0, percent);\n  Motor10.setVelocity(100.0, percent);\n  return 0;\n}\n\n// \"when I receive Up\" hat block\nvoid onevent_Up_0() {\n  Motor1.spin(forward);\n}\n\n// \"when I receive Up\" hat block\nvoid onevent_Up_1() {\n  Motor11.spin(forward);\n}\n\n// \"when I receive Up\" hat block\nvoid onevent_Up_2() {\n  Motor10.spin(forward);\n}\n\n// \"when I receive Up\" hat block\nvoid onevent_Up_3() {\n  Motor20.spin(forward);\n}\n\n// \"when I receive Down\" hat block\nvoid onevent_Down_0() {\n  Motor11.spin(reverse);\n}\n\n// \"when I receive Down\" hat block\nvoid onevent_Down_1() {\n  Motor1.spin(reverse);\n}\n\n// \"when I receive Down\" hat block\nvoid onevent_Down_2() {\n  Motor10.spin(reverse);\n}\n\n// \"when I receive Down\" hat block\nvoid onevent_Down_3() {\n  Motor20.spin(reverse);\n}\n\n// \"when I receive Right\" hat block\nvoid onevent_Right_0() {\n  Motor1.spin(forward);\n}\n\n// \"when I receive Right\" hat block\nvoid onevent_Right_1() {\n  Motor10.spin(reverse);\n}\n\n// \"when I receive Right\" hat block\nvoid onevent_Right_2() {\n  Motor11.spin(reverse);\n}\n\n// \"when I receive Right\" hat block\nvoid onevent_Right_3() {\n  Motor20.spin(forward);\n}\n\n// \"when I receive Left\" hat block\nvoid onevent_Left_0() {\n  Motor1.spin(reverse);\n}\n\n// \"when I receive Left\" hat block\nvoid onevent_Left_1() {\n  Motor11.spin(forward);\n}\n\n// \"when I receive Left\" hat block\nvoid onevent_Left_2() {\n  Motor10.spin(forward);\n}\n\n// \"when I receive Left\" hat block\nvoid onevent_Left_3() {\n  Motor20.spin(reverse);\n}\n\n// \"when I receive Clockwise\" hat block\nvoid onevent_Clockwise_0() {\n  Motor1.spin(forward);\n}\n\n// \"when I receive Clockwise\" hat block\nvoid onevent_Clockwise_1() {\n  Motor10.spin(reverse);\n}\n\n// \"when I receive Clockwise\" hat block\nvoid onevent_Clockwise_2() {\n  Motor11.spin(forward);\n}\n\n// \"when I receive Clockwise\" hat block\nvoid onevent_Clockwise_3() {\n  Motor20.spin(reverse);\n}\n\n// \"when I receive Counter_Clockwise\" hat block\nvoid onevent_Counter_Clockwise_0() {\n  Motor1.spin(reverse);\n}\n\n// \"when I receive Counter_Clockwise\" hat block\nvoid onevent_Counter_Clockwise_1() {\n  Motor10.spin(forward);\n}\n\n// \"when I receive Counter_Clockwise\" hat block\nvoid onevent_Counter_Clockwise_2() {\n  Motor11.spin(reverse);\n}\n\n// \"when I receive Counter_Clockwise\" hat block\nvoid onevent_Counter_Clockwise_3() {\n  Motor20.spin(forward);\n}\n\n// \"when I receive STOP\" hat block\nvoid onevent_STOP_0() {\n  Motor1.stop();\n}\n\n// \"when I receive STOP\" hat block\nvoid onevent_STOP_1() {\n  Motor10.stop();\n}\n\n// \"when I receive STOP\" hat block\nvoid onevent_STOP_2() {\n  Motor11.stop();\n}\n\n// \"when I receive STOP\" hat block\nvoid onevent_STOP_3() {\n  Motor20.stop();\n}\n\n\nint main() {\n  // register event handlers\n  Up(onevent_Up_0);\n  Up(onevent_Up_1);\n  Up(onevent_Up_2);\n  Up(onevent_Up_3);\n  Down(onevent_Down_0);\n  Down(onevent_Down_1);\n  Down(onevent_Down_2);\n  Down(onevent_Down_3);\n  Right(onevent_Right_0);\n  Right(onevent_Right_1);\n  Right(onevent_Right_2);\n  Right(onevent_Right_3);\n  Left(onevent_Left_0);\n  Left(onevent_Left_1);\n  Left(onevent_Left_2);\n  Left(onevent_Left_3);\n  Clockwise(onevent_Clockwise_0);\n  Clockwise(onevent_Clockwise_1);\n  Clockwise(onevent_Clockwise_2);\n  Clockwise(onevent_Clockwise_3);\n  Counter_Clockwise(onevent_Counter_Clockwise_0);\n  Counter_Clockwise(onevent_Counter_Clockwise_1);\n  Counter_Clockwise(onevent_Counter_Clockwise_2);\n  Counter_Clockwise(onevent_Counter_Clockwise_3);\n  STOP(onevent_STOP_0);\n  STOP(onevent_STOP_1);\n  STOP(onevent_STOP_2);\n  STOP(onevent_STOP_3);\n\n  wait(15, msec);\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // wait for rotation sensor to fully initialize\n  wait(30, msec);\n\n  whenStarted1();\n}","target":"Physical"}